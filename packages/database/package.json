  },
  "devDependencies": {
    "@faker-js/faker": "9.4.0",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "22.10.10",
    "prisma": "6.2.1",
    "prisma-erd-generator": "1.11.2",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "zod-prisma-types": "3.2.1"
  },
  "dependencies": {
    "@prisma/client": "6.2.1",
    "zod": "^3.24.1"
  }
}
