{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["./.env"],
  "globalEnv": ["API_URL"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "type-check": {},
    "format": {},
    "check": {},
    "test": {},
    "test:cov": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "gen:ui": {
      "cache": false,
      "interactive": true
    }
  },
  "ui": "tui"
}
